<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="SwagCookieConsentManager\Bundle\CookieBundle\Services\CookieCollector">
            <argument id="events" type="service" />
            <argument id="snippets" type="service" />
        </service>

        <service id="SwagCookieConsentManager\Bundle\CookieBundle\Services\CookieHandler">
            <argument id="SwagCookieConsentManager\Bundle\CookieBundle\Services\CookieCollector" type="service" />
        </service>
    </services>
</container>
